{
	"name": "@wrb/web",
	"version": "0.9.0",
	"private": true,
	"description": "Electron + React + TS + WL",
	"repository": "git+https://github.com/ToneAr/electron-react-wl-boilerplate",
	"author": "Antonis Aristeidou <tonya@wolfram.com>",
	"contributors": [],
	"main": "build/index.html",
	"source": "src/renderer/index.html",
	"domain": "wreltech.wolfram.com",
	"scripts": {
		"format": "prettier --write .",
		"lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",

		"typecheck:node": "tsc --noEmit -p ../../tsconfig.node.json --composite false",
		"typecheck:web": "tsc --noEmit -p ../../tsconfig.web.json --composite false",
		"typecheck": "yarn typecheck:node && yarn typecheck:web",

		"start": "concurrently \"yarn start:server\" \"yarn start:client\"",
		"start:client": "vite",
		"start:server": "nodemon",

		"dev": "concurrently \"yarn dev:server\" \"yarn dev:client\"",
		"dev:client": "cross-env NODE_ENV=development vite dev",
		"dev:server": "cross-env NODE_ENV=development nodemon",

		"build": "yarn typecheck && vite build",
		"preview": "yarn build && concurrently \"yarn dev:server\" \"vite preview\""
	},
	"dependencies": {
		"express": "^4.21.0",
		"nodemon": "^3.1.4",
		"react": "*",
		"react-dom": "*",
		"socket.io": "^4.7.5",
		"socket.io-client": "^4.7.5",
		"vite-tsconfig-paths": "^5.0.1"
	},
	"devDependencies": {
		"vite": "^5.4.5",
		"@types/express": "^4.17.21",
		"@types/socket.io": "^3.0.2",
		"@types/socket.io-client": "^3.0.0"
	},
	"peerDependencies": {
		"@electron-toolkit/eslint-config-prettier": "*",
		"@electron-toolkit/eslint-config-ts": "*",
		"@electron-toolkit/preload": "*",
		"@electron-toolkit/tsconfig": "*",
		"@electron-toolkit/utils": "*",
		"@eslint/eslintrc": "*",
		"@eslint/js": "*",
		"@testing-library/jest-dom": "*",
		"@testing-library/react": "*",
		"@types/jest": "*",
		"@types/node": "*",
		"@types/react": "*",
		"@types/react-dom": "*",
		"@types/react-test-renderer": "*",
		"@typescript-eslint/eslint-plugin": "*",
		"@typescript-eslint/parser": "*",
		"@wrb/frontend": "*",
		"@wrb/wl": "*",
		"eslint": "*",
		"eslint-config-airbnb-base": "*",
		"eslint-import-resolver-typescript": "*",
		"eslint-plugin-compat": "*",
		"eslint-plugin-import": "*",
		"eslint-plugin-jest": "*",
		"eslint-plugin-jsx-a11y": "*",
		"eslint-plugin-promise": "*",
		"eslint-plugin-react": "*",
		"eslint-plugin-react-hooks": "*",
		"globals": "*",
		"jest": "*",
		"prettier": "*",
		"react": "*",
		"react-dom": "*",
		"react-router-dom": "*",
		"ts-node": "*",
		"tsx": "*"
	},
	"license": "MIT"
}
