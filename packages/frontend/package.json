{
	"name": "@wrb/frontend",
	"version": "1.0.0",
	"description": "WRB Sub-package: Frontend",
	"repository": "git+https://github.com/ToneAr/wolfram-react-boilerplate",
	"author": "Antonis Aristeidou <tonya@wolfram.com>",
	"main": "build/index.cjs.js",
	"module": "build/index.es.js",
	"types": "build/index.d.ts",
	"source": "src/index.ts",
	"private": true,
	"scripts": {
		"format": "prettier --write .",
		"lint": "eslint . --fix",
		"test": "jest",
		"typecheck": "tsc --noEmit -p ../../tsconfig.web.json --composite false",
		"dev": "cross-env NODE_ENV=development concurrently \"yarn workspace @wrb/web start:server\" \"vite\"",
		"build": "yarn typecheck && vite build"
	},
	"jest": {
		"moduleDirectories": [
			"node_modules",
			"packages"
		],
		"moduleFileExtensions": [
			"js",
			"jsx",
			"ts",
			"tsx",
			"json"
		],
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
				"<rootDir>/src/mocks/fileMock.js",
			"\\.(css|less|sass|scss)$": "identity-obj-proxy",
			"\\?react$": "<rootDir>/src/mocks/fileMock.js"
		},
		"setupFiles": [],
		"testEnvironment": "jsdom",
		"testEnvironmentOptions": {
			"url": "http://localhost/"
		},
		"testPathIgnorePatterns": [
			"build/*"
		],
		"transform": {
			"\\.(ts|tsx|js|jsx)$": "ts-jest"
		}
	},
	"dependencies": {
		"@emotion/react": "^11.13.3",
		"@emotion/styled": "^11.13.0",
		"@mui/material": "^6.1.0",
		"axios": "^1.7.7"
	},
	"devDependencies": {
		"@rollup/plugin-babel": "^6.0.4",
		"@rollup/plugin-commonjs": "^26.0.1",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-terser": "^0.4.4",
		"@rollup/plugin-typescript": "^11.1.6",
		"@types/react-router-dom": "^5.3.3",
		"concurrently": "^8.2.1",
		"jest-environment-jsdom": "^29.7.0",
		"rollup": "^4.18.1",
		"rollup-plugin-dts": "^6.1.1",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-postcss": "^4.0.2",
		"ts-jest": "^29.2.5",
		"tslib": "^2.6.3",
		"vite": "^5.4.5",
		"@testing-library/jest-dom": "^6.1.3",
		"@testing-library/react": "^14.0.0",
		"@types/jest": "^29.5.5",
		"jest": "^29.7.0"
	},
	"peerDependancies": {
		"vite-plugin-svgr": "*",
		"vite-plugin-dts": "*",
		"@vitejs/plugin-react": "*",
		"@vituum/vite-plugin-postcss": "*",
		"react": "*",
		"react-dom": "*",
		"react-router-dom": "*",
		"typescript": "*",
		"cross-env": "*",
		"@electron-toolkit/eslint-config-prettier": "*",
		"@electron-toolkit/eslint-config-ts": "*",
		"@electron-toolkit/preload": "*",
		"@electron-toolkit/tsconfig": "*",
		"@electron-toolkit/utils": "*",
		"@eslint/eslintrc": "*",
		"@eslint/js": "*",
		"@types/node": "*",
		"@types/react": "*",
		"@types/react-dom": "*",
		"@types/react-test-renderer": "*",
		"@typescript-eslint/eslint-plugin": "*",
		"@typescript-eslint/parser": "*",
		"eslint": "*",
		"eslint-import-resolver-typescript": "*",
		"identity-obj-proxy": "*",
		"eslint-plugin-compat": "*",
		"eslint-plugin-import": "*",
		"eslint-plugin-jest": "*",
		"eslint-plugin-jsx-a11y": "*",
		"eslint-plugin-promise": "*",
		"eslint-plugin-react": "*",
		"eslint-plugin-react-hooks": "*",
		"globals": "*",
		"prettier": "*",
		"tsx": "*"
	},
	"license": "MIT"
}
