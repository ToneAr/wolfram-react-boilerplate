{
	"name": "@wrb/frontend",
	"version": "0.9.0",
	"description": "Electron + React + TS + WL",
	"repository": "git+https://github.com/ToneAr/electron-react-wl-boilerplate",
	"author": "Antonis Aristeidou <tonya@wolfram.com>",
	"main": "build/index.cjs.js",
	"module": "build/index.es.js",
	"types": "build/index.d.ts",
	"source": "src/index.ts",
	"private": true,
	"scripts": {
		"format": "prettier --write .",
		"lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
		"typecheck:web": "tsc --noEmit -p ../../tsconfig.web.json --composite false",
		"typecheck": "yarn typecheck:web",
		"dev": "concurrently \"yarn workspace @wrb/web start:server\" \"cross-env NODE_ENV=development vite\"",
		"build": "vite build"
	},
	"dependencies": {
		"@emotion/react": "^11.13.3",
		"@emotion/styled": "^11.13.0",
		"@mui/material": "^6.1.0",
		"@types/react-router-dom": "^5.3.3",
		"@vitejs/plugin-react": "^4.3.1",
		"axios": "^1.7.7",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^6.26.2",
		"tsc": "^2.0.4"
	},
	"devDependencies": {
		"@rollup/plugin-babel": "^6.0.4",
		"@rollup/plugin-commonjs": "^26.0.1",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-terser": "^0.4.4",
		"@rollup/plugin-typescript": "^11.1.6",
		"@vituum/vite-plugin-postcss": "^1.1.0",
		"rollup": "^4.18.1",
		"concurrently": "^8.2.1",
		"rollup-plugin-dts": "^6.1.1",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-postcss": "^4.0.2",
		"tslib": "^2.6.3",
		"vite": "^5.4.5",
		"vite-plugin-dts": "^4.2.1",
		"vite-plugin-svgr": "^4.2.0"
	},
	"peerDependancies": {
		"@wrb/hooks": "*",
		"typescript": "*",
		"@electron-toolkit/eslint-config-prettier": "*",
		"@electron-toolkit/eslint-config-ts": "*",
		"@electron-toolkit/preload": "*",
		"@electron-toolkit/tsconfig": "*",
		"@electron-toolkit/utils": "*",
		"@eslint/eslintrc": "*",
		"@eslint/js": "*",
		"@testing-library/jest-dom": "*",
		"@testing-library/react": "*",
		"@types/jest": "*",
		"@types/node": "*",
		"@types/react": "*",
		"@types/react-dom": "*",
		"@types/react-test-renderer": "*",
		"@typescript-eslint/eslint-plugin": "*",
		"@typescript-eslint/parser": "*",
		"eslint": "*",
		"eslint-config-airbnb-base": "*",
		"eslint-import-resolver-typescript": "*",
		"eslint-plugin-compat": "*",
		"eslint-plugin-import": "*",
		"eslint-plugin-jest": "*",
		"eslint-plugin-jsx-a11y": "*",
		"eslint-plugin-promise": "*",
		"eslint-plugin-react": "*",
		"eslint-plugin-react-hooks": "*",
		"globals": "*",
		"jest": "*",
		"prettier": "*",
		"tsx": "*"
	},
	"license": "MIT"
}
